{
  "name": "react-ssr-skeleton",
  "lint-staged": {
    "pages/**/*.{js,jsx}": [
      "eslint",
      "git add"
    ],
    "components/**/*.{js,jsx}": [
      "eslint",
      "git add"
    ]
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "rimraf .next && rimraf out && npm run build && next export -o dist",
    "analyze:size": "cross-env ANALYZE=SIZE next build",
    "analyze:bundles": "cross-env ANALYZE=BUNDLES next build",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "next": "^7.0.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@types/next": "^7.0.4",
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.9",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-source-maps": "0.0.3",
    "@zeit/next-typescript": "^1.1.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-config-fbjs": "^2.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-relay": "0.0.28",
    "node-sass": "^4.10.0",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-preset-env": "^6.4.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-bundle-size-analyzer": "^2.7.0"
  }
}
